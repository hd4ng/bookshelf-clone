{
  "name": "bookshelf-clone",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bookshelf-clone.netlify.com/",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/dialog": "^0.10.0",
    "@reach/menu-button": "^0.10.0",
    "@reach/tooltip": "^0.10.0",
    "@reach/visually-hidden": "^0.10.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.0.2",
    "bootstrap": "^4.4.1",
    "match-sorter": "^4.1.0",
    "msw": "^0.15.9",
    "node-match-path": "^0.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.2.1",
    "react-icons": "^3.10.0",
    "react-query": "^1.4.4",
    "react-query-devtools": "^1.1.5",
    "react-router": "^6.0.0-alpha.4",
    "react-router-dom": "^6.0.0-alpha.4",
    "react-scripts": "^3.4.1",
    "stop-runaway-react-effects": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/match-sorter": "^4.0.0",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "serve": "^11.3.0",
    "start-server-and-test": "^1.11.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "npm run test -- --watchAll false --coverage",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "format": "prettier --write \"**/*.+(js|ts|tsx|json|css|md|mdx|html)\"",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel lint format check-types test:coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!<rootDir>/node_modules/**/*",
      "!<rootDir>/src/test/**/*",
      "!<rootDir>/src/setupProxy.js",
      "!<rootDir>/src/setupTests.js",
      "!<rootDir>/src/dev-tools/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
